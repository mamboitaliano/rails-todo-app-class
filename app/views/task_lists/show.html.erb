<span><%= @task_list.name %></span>
<span>
<%= link_to 'Add Task', new_task_list_task_path(@task_list) %>
</span>
<% @tasks.each do |task| %>
<br>
  <%= task.description %>
  <%= task.completed %>
  <%= link_to 'Complete', completed_task_list_task_path(@task_list, task) %>
  <%= link_to 'Edit', edit_task_list_task_path(@task_list, task) %>
  <%= link_to 'Delete', task_list_task_path(@task_list, task), method: :delete %>
<% end %>
